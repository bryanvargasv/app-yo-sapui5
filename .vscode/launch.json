{
    "inputs": [
        {
            "id": "UI5VersionV4",
            "type": "command",
            "command": "sap.ux.appGenerator.ui5VersionV4"
        }
    ],
    "configurations": [
        {
            "name": "app-runui5",
            "cwd": "${workspaceFolder}",
            "runtimeArgs": [
                "run-script",
                "start"
            ],
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "args": [
                "--",
                "--ui5"
            ],
            "windows": {
                "runtimeExecutable": "npm.cmd",
                "args": [
                    "--",
                    "--ui5"
                ]
            },
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"c:\\\\sapui5-vsc\\\\app-yo\"}"
            }
        },
        {
            "name": "Start student",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "cwd": "${workspaceFolder}/student",
            "windows": {
                "runtimeExecutable": "npm.cmd"
            },
            "runtimeArgs": [
                "run-script",
                "start"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "port": 9229,
            "env": {
                "DEBUG": "--inspect"
            }
        },
        {
            "name": "Start student with UI5 Version",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "cwd": "${workspaceFolder}/student",
            "windows": {
                "runtimeExecutable": "npm.cmd"
            },
            "runtimeArgs": [
                "run-script",
                "start"
            ],
            "args": [
                "--",
                "${input:UI5VersionV4}"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "port": 9229,
            "env": {
                "DEBUG": "--inspect"
            }
        },
        {
            "name": "Start student Local",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/student",
            "runtimeExecutable": "npm",
            "windows": {
                "runtimeExecutable": "npm.cmd"
            },
            "runtimeArgs": [
                "run-script",
                "start-local"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "port": 9229,
            "env": {}
        }
    ],
    "compounds": [],
    "version": "0.2.0"
}